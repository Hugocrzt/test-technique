{"ast":null,"code":"import BannerLarge from \"@/components/BannerLarge\";\nimport BannerFaq from \"@/components/BannerFaq\";\nimport CartPopin from \"@/components/CartPopin\";\nexport default {\n  name: \"HomePage\",\n  components: {\n    BannerLarge,\n    BannerFaq,\n    CartPopin\n  },\n\n  mounted() {\n    this.ChangeColorHeader();\n  },\n\n  methods: {\n    // Toggle Menu Color Landing\n    ChangeColorHeader() {\n      const HeaderContainer = document.querySelector(\".header-container\");\n      const LandingSection = document.querySelector(\".banner-large-section\");\n      gsap.to(HeaderContainer, {\n        scrollTrigger: {\n          trigger: LandingSection,\n          start: \"top bottom\",\n          end: \"bottom 0%\",\n          toggleClass: {\n            targets: HeaderContainer,\n            className: \"is-active\"\n          }\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA,OAAOA,WAAP,MAAwB,0BAAxB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVJ,eADU;AAEVC,aAFU;AAGVC;AAHU,GAFC;;AAObG,SAAO,GAAG;AACR,SAAKC,iBAAL;AACD,GATY;;AAUbC,SAAO,EAAE;AACP;AACAD,qBAAiB,GAAG;AAClB,YAAME,eAAc,GAAIC,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,CAAxB;AACA,YAAMC,cAAa,GAAIF,QAAQ,CAACC,aAAT,CAAuB,uBAAvB,CAAvB;AACAE,UAAI,CAACC,EAAL,CAAQL,eAAR,EAAyB;AACvBM,qBAAa,EAAE;AACbC,iBAAO,EAAEJ,cADI;AAEbK,eAAK,EAAE,YAFM;AAGbC,aAAG,EAAE,WAHQ;AAIbC,qBAAW,EAAE;AAAEC,mBAAO,EAAEX,eAAX;AAA4BY,qBAAS,EAAE;AAAvC;AAJA;AADQ,OAAzB;AAQD;;AAbM;AAVI,CAAf","names":["BannerLarge","BannerFaq","CartPopin","name","components","mounted","ChangeColorHeader","methods","HeaderContainer","document","querySelector","LandingSection","gsap","to","scrollTrigger","trigger","start","end","toggleClass","targets","className"],"sourceRoot":"","sources":["/Users/hugocrozet/Documents/Website/test-technique/src/pages/Faq.vue"],"sourcesContent":["<template>\n  <div>\n    <BannerLarge />\n    <BannerFaq />\n    <CartPopin />\n  </div>\n</template>