{"ast":null,"code":"import { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\nexport default {\n  name: \"BannerLarge\",\n\n  created() {\n    this.fetchAllProducts();\n  },\n\n  computed: { ...mapState([\"products\"])\n  },\n  methods: { ...mapActions([\"fetchAllProducts\"]),\n\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      new Swiper(\".swiper\", {\n        loop: true,\n        speed: 1000,\n        slidesPerView: 4,\n        spaceBetween: 20\n      });\n    }\n\n  },\n  mounted: function () {\n    this.SwiperCarrousel();\n  }\n};","map":{"version":3,"mappings":"AAyCA,SAASA,UAAT,EAAqBC,QAArB,QAAqC,MAArC;AACA,OAAOC,MAAP,MAAmB,QAAnB;AACA,OAAO,YAAP;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;;AAGbC,SAAO,GAAG;AACR,SAAKC,gBAAL;AACD,GALY;;AAObC,UAAQ,EAAE,EACR,GAAGL,QAAQ,CAAC,CAAC,UAAD,CAAD;AADH,GAPG;AAWbM,SAAO,EAAE,EACP,GAAGP,UAAU,CAAC,CAAC,kBAAD,CAAD,CADN;;AAEP;AACAQ,mBAAe,GAAG;AAChB,UAAIN,MAAJ,CAAW,SAAX,EAAsB;AACpBO,YAAI,EAAE,IADc;AAEpBC,aAAK,EAAE,IAFa;AAGpBC,qBAAa,EAAE,CAHK;AAIpBC,oBAAY,EAAE;AAJM,OAAtB;AAMD;;AAVM,GAXI;AAwBbC,SAAO,EAAE,YAAY;AACnB,SAAKL,eAAL;AACD;AA1BY,CAAf","names":["mapActions","mapState","Swiper","name","created","fetchAllProducts","computed","methods","SwiperCarrousel","loop","speed","slidesPerView","spaceBetween","mounted"],"sourceRoot":"","sources":["/Users/hugocrozet/Documents/Website/test-technique/src/components/BannerCarrousel.vue"],"sourcesContent":["<template>\n  <section class=\"banner-caroussel-section\">\n    <div class=\"banner-caroussel-container\">\n      <div class=\"subtitle-left-wrapper\">\n        <h2>Notre Sélection</h2>\n      </div>\n      <div class=\"banner-caroussel-wrapper\">\n        <div v-if=\"product.image\" class=\"swiper product-container\">\n          <div class=\"swiper-wrapper products-wrapper\">\n            <div\n              v-for=\"product in products\"\n              :key=\"product.id\"\n              class=\"swiper-slide\"\n            >\n              <router-link to=\"#product\" class=\"products-space-image\">\n                <img class=\"product-image\" :src=\"product.image\" alt=\"\" />\n                <img\n                  class=\"rollover-product-image\"\n                  :src=\"product.image\"\n                  alt=\"\"\n                />\n              </router-link>\n              <div class=\"products-space-text\">\n                <router-link to=\"#product\"\n                  ><p class=\"product-title\">{{ product.title }}</p></router-link\n                >\n                <router-link to=\"#product\"\n                  ><p class=\"product-price\">\n                    {{ product.price }} €\n                  </p></router-link\n                >\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\n\nexport default {\n  name: \"BannerLarge\",\n\n  created() {\n    this.fetchAllProducts();\n  },\n\n  computed: {\n    ...mapState([\"products\"]),\n  },\n\n  methods: {\n    ...mapActions([\"fetchAllProducts\"]),\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      new Swiper(\".swiper\", {\n        loop: true,\n        speed: 1000,\n        slidesPerView: 4,\n        spaceBetween: 20,\n      });\n    },\n  },\n\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n};\n</script>\n\n<style lang=\"css\" src=\"@/assets/css/banner_carrousel.css\"></style>\n"]},"metadata":{},"sourceType":"module"}