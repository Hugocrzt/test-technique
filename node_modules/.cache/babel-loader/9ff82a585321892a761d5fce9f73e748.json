{"ast":null,"code":"import BannerLarge from \"@/components/BannerLarge\";\nimport BannerFaq from \"@/components/BannerFaq\";\nimport CartPopin from \"@/components/CartPopin\";\nexport default {\n  name: \"HomePage\",\n  components: {\n    BannerLarge,\n    BannerFaq,\n    CartPopin\n  },\n\n  mounted() {\n    this.ChangeColorHeader();\n  },\n\n  methods: {\n    // Toggle Menu Color Landing\n    ChangeColorHeader() {\n      window.scrollTo(0, 0);\n      const HeaderContainer = document.querySelector(\".header-container\");\n      const LandingSection = document.querySelector(\".banner-large-section\");\n      window.addEventListener(\"scroll\", function () {\n        const distanceToTop = LandingSection.getBoundingClientRect().top;\n\n        if (distanceToTop < 0) {\n          HeaderContainer.classList.remove(\"is-active\");\n        } else {\n          HeaderContainer.classList.add(\"is-active\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA,OAAOA,WAAP,MAAwB,0BAAxB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVJ,eADU;AAEVC,aAFU;AAGVC;AAHU,GAFC;;AAObG,SAAO,GAAG;AACR,SAAKC,iBAAL;AACD,GATY;;AAUbC,SAAO,EAAE;AACP;AACAD,qBAAiB,GAAG;AAClBE,YAAM,CAACC,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;AACA,YAAMC,eAAc,GAAIC,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,CAAxB;AACA,YAAMC,cAAa,GAAIF,QAAQ,CAACC,aAAT,CAAuB,uBAAvB,CAAvB;AAEAJ,YAAM,CAACM,gBAAP,CAAwB,QAAxB,EAAkC,YAAY;AAC5C,cAAMC,aAAY,GAAIF,cAAc,CAACG,qBAAf,GAAuCC,GAA7D;;AACA,YAAIF,aAAY,GAAI,CAApB,EAAuB;AACrBL,yBAAe,CAACQ,SAAhB,CAA0BC,MAA1B,CAAiC,WAAjC;AACF,SAFA,MAEO;AACLT,yBAAe,CAACQ,SAAhB,CAA0BE,GAA1B,CAA8B,WAA9B;AACF;AACD,OAPD;AAQD;;AAfM;AAVI,CAAf","names":["BannerLarge","BannerFaq","CartPopin","name","components","mounted","ChangeColorHeader","methods","window","scrollTo","HeaderContainer","document","querySelector","LandingSection","addEventListener","distanceToTop","getBoundingClientRect","top","classList","remove","add"],"sourceRoot":"","sources":["/Users/hugocrozet/Documents/Website/test-technique/src/pages/Faq.vue"],"sourcesContent":["<template>\n  <div>\n    <BannerLarge />\n    <BannerFaq />\n    <CartPopin />\n  </div>\n</template>\n\n<script>\nimport BannerLarge from \"@/components/BannerLarge\";\nimport BannerFaq from \"@/components/BannerFaq\";\nimport CartPopin from \"@/components/CartPopin\";\n\nexport default {\n  name: \"HomePage\",\n  components: {\n    BannerLarge,\n    BannerFaq,\n    CartPopin,\n  },\n  mounted() {\n    this.ChangeColorHeader();\n  },\n  methods: {\n    // Toggle Menu Color Landing\n    ChangeColorHeader() {\n      window.scrollTo(0, 0);\n      const HeaderContainer = document.querySelector(\".header-container\");\n      const LandingSection = document.querySelector(\".banner-large-section\");\n\n      window.addEventListener(\"scroll\", function () {\n        const distanceToTop = LandingSection.getBoundingClientRect().top;\n        if (distanceToTop < 0) {\n          HeaderContainer.classList.remove(\"is-active\");\n        } else {\n          HeaderContainer.classList.add(\"is-active\");\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"css\" src=\"@/assets/css/main.css\"></style>\n"]},"metadata":{},"sourceType":"module"}