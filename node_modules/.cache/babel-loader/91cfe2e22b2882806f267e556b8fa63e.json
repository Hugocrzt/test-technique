{"ast":null,"code":"import { gsap } from \"gsap\";\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\nimport { mapState } from \"vuex\";\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"BannerLarge\",\n\n  mounted() {\n    this.MobileCheckedMenuAdaptation();\n    this.ChangeColorHeader();\n    setTimeout(() => {\n      this.ChangeColorHeader();\n    }, 1000);\n  },\n\n  methods: {\n    MobileCheckedMenuAdaptation: function () {\n      const MenuToggle = document.querySelector(\"#menu-toggle\");\n      const MenuBtn = document.querySelector(\".menu-open\");\n      const HeaderLogo = document.querySelector(\".header-logo-wrapper a img\");\n      const HeaderCart = document.querySelector(\".header-cart-wrapper .header-cart-btn img\");\n      MenuBtn.addEventListener(\"click\", toggleCheckedClass);\n\n      function toggleCheckedClass() {\n        console.log(\"test\");\n\n        if (MenuToggle.checked) {\n          HeaderLogo.classList.remove(\"checked\");\n          HeaderCart.classList.remove(\"checked\");\n        } else {\n          HeaderLogo.classList.add(\"checked\");\n          HeaderCart.classList.add(\"checked\");\n        }\n      }\n    },\n    ChangeColorHeader: function () {\n      const HeaderContainer = document.querySelector(\".header-container\");\n      const LandingSection = document.querySelector(\".banner-large-section\");\n      console.log(LandingSection);\n      gsap.to(HeaderContainer, {\n        scrollTrigger: {\n          trigger: LandingSection,\n          start: \"top bottom\",\n          end: \"bottom 0%\",\n          toggleClass: {\n            targets: HeaderContainer,\n            className: \"is-active\"\n          }\n        }\n      });\n    }\n  },\n  computed: { ...mapState([\"cart\"])\n  }\n};","map":{"version":3,"mappings":"AA+DA,SAASA,IAAT,QAAqB,MAArB;AACA,SAASC,aAAT,QAA8B,oBAA9B;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEAF,IAAI,CAACG,cAAL,CAAoBF,aAApB;AAEA,eAAe;AACbG,MAAI,EAAE,aADO;;AAGbC,SAAO,GAAG;AACR,SAAKC,2BAAL