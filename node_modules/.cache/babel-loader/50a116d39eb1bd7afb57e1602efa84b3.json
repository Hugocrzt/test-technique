apper products-wrapper\">\n            <div\n              v-for=\"product in products\"\n              :key=\"product.id\"\n              class=\"swiper-slide\"\n            >\n              <router-link to=\"#product\" class=\"products-space-image\">\n                <img :src=\"product.image\" alt=\"\" />\n              </router-link>\n              <div class=\"products-space-text\">\n                <router-link to=\"#product\"\n                  ><p class=\"product-title\">{{ product.title }}</p></router-link\n                >\n                <router-link to=\"#product\"\n                  ><p class=\"product-price\">\n                    {{ product.price }} €\n                  </p></router-link\n                >\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport { gsap } from \"gsap\";\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\n\ngsap.registerPlugin(ScrollTrigger);\n\nexport default {\n  name: \"BannerLarge\",\n\n  computed: {\n    ...mapState([\"products\"]),\n  },\n\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n\n  methods: {\n    SwiperCarrousel() {\n      new Swiper(\".swiper\", {\n        loop: true,\n        speed: 1000,\n        slidesPerView: 4,\n        spaceBetween: 20,\n      });\n    },\n    ...mapActions([\"fetchAllProducts\"]),\n  },\n\n  created() {\n    this.fetchAllProducts();\n  },\n};\n</script>\n\n<style lang=\"css\" src=\"@/assets/css/banner_carrousel.css\"></style>\n"]},"metadata":{},"sourceType":"module"}