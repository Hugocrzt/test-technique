{"ast":null,"code":"import { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\nexport default {\n  name: \"BannerLarge\",\n  computed: { ...mapState([\"products\"])\n  },\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n  methods: {\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      if (this.products.length !== 0) {\n        initSwiper;\n      } else {\n        setTimeout(initSwiper, 2000);\n      }\n\n      function initSwiper() {\n        new Swiper(\".swiper\", {\n          loop: true,\n          speed: 1000,\n          noSwiping: true,\n          noSwipingClass: \"swiper-wrapper\",\n          slidesPerView: 4,\n          spaceBetween: 20\n        });\n      }\n    },\n\n    ...mapActions([\"fetchAllProducts\"])\n  },\n\n  created() {\n    this.fetchAllProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAyCA,SAASA,UAAT,EAAqBC,QAArB,QAAqC,MAArC;AACA,OAAOC,MAAP,MAAmB,QAAnB;AACA,OAAO,YAAP;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAGbC,UAAQ,EAAE,EACR,GAAGH,QAAQ,CAAC,CAAC,UAAD,CAAD;AADH,GAHG;AAObI,SAAO,EAAE,YAAY;AACnB,SAAKC,eAAL;AACD,GATY;AAWbC,SAAO,EAAE;AACP;AACAD,mBAAe,GAAG;AAChB,UAAI,KAAKE,QAAL,CAAcC,MAAd,KAAyB,CAA7B,EAAgC;AAC9BC,kBAAU;AACZ,OAFA,MAEO;AACLC,kBAAU,CAACD,UAAD,EAAa,IAAb,CAAV;AACF;;AAEA,eAASA,UAAT,GAAsB;AACpB,YAAIR,MAAJ,CAAW,SAAX,EAAsB;AACpBU,cAAI,EAAE,IADc;AAEpBC,eAAK,EAAE,IAFa;AAGpBC,mBAAS,EAAE,IAHS;AAIpBC,wBAAc,EAAE,gBAJI;AAKpBC,uBAAa,EAAE,CALK;AAMpBC,sBAAY,EAAE;AANM,SAAtB;AAQF;AACD,KAnBM;;AAoBP,OAAGjB,UAAU,CAAC,CAAC,kBAAD,CAAD;AApBN,GAXI;;AAkCbkB,SAAO,GAAG;AACR,SAAKC,gBAAL;AACD;;AApCY,CAAf","names":["mapActions","mapState","Swiper","name","computed","mounted","SwiperCarrousel","methods","products","length","initSwiper","setTimeout","loop","speed","noSwiping","noSwipingClass","slidesPerView","spaceBetween","created","fetchAllProducts"],"sourceRoot":"","sources":["/Users/hugocrozet/Documents/Website/test-technique/src/components/BannerCarrousel.vue"],"sourcesContent":["<template>\n  <sectio