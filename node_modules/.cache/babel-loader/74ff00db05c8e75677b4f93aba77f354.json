{"ast":null,"code":"import { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\nexport default {\n  name: \"BannerLarge\",\n  computed: { ...mapState([\"products\"])\n  },\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n  methods: {\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      const that = this;\n\n      function SwiperInit() {\n        new Swiper(\".swiper\", {\n          // when window width is < 767px (Mobile)\n          loop: true,\n          speed: 1000,\n          slidesPerView: 1,\n          spaceBetween: 20,\n          pagination: {\n            el: \".swiper-pagination\",\n            type: \"bullets\"\n          },\n          breakpoints: {\n            // when window width is >= 1023px (Desktop)\n            1023: {\n              slidesPerView: 4,\n              spaceBetween: 20,\n              noSwiping: true,\n              noSwipingClass: \"swiper-wrapper\"\n            },\n            // when window width is >= 767px && < 1023 (Tablette)\n            767: {\n              slidesPerView: 2,\n              spaceBetween: 20\n            }\n          }\n        });\n      }\n\n      const SwiperInitInterval = setInterval(function () {\n        if (that.products.length !== 0) {\n          SwiperInit();\n          clearInterval(SwiperInitInterval);\n        }\n      }, 100);\n    },\n\n    ...mapActions([\"fetchAllProducts\"])\n  },\n\n  created() {\n    this.fetchAllProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAyCA,SAASA,UAAT,EAAqBC,QAArB,QAAqC,MAArC;AACA,OAAOC,MAAP,MAAmB,QAAnB;AACA,OAAO,YAAP;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAGbC,UAAQ,EAAE,EACR,GAAGH,QAAQ,CAAC,CAAC,UAAD,CAAD;AADH,GAHG;AAObI,SAAO,EAAE,YAAY;AACnB,SAAKC,eAAL;AACD,GATY;AAWbC,SAAO,EAAE;AACP;AACAD,mBAAe,GAAG;AAChB,YAAME,IAAG,GAAI,IAAb;;AAEA,eAASC,UAAT,GAAsB;AACpB,YAAIP,MAAJ,CAAW,SAAX,EAAsB;AACpB;AACAQ,cAAI,EAAE,IAFc;AAGpBC,eAAK,EAAE,IAHa;AAIpBC,uBAAa,EAAE,CAJK;AAKpBC,sBAAY,EAAE,EALM;AAMpBC,oBAAU,EAAE;AACVC,cAAE,