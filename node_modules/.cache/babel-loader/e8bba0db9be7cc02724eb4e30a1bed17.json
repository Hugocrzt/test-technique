{"ast":null,"code":"export default {\n  name: \"CartPopin\",\n  mounted: function () {\n    this.ToogleCart();\n  },\n  methods: {\n    // Toggle Cart Interface\n    ToogleCart() {\n      const CartContainer = document.querySelector(\".cart-popin-container\");\n      const CartVoile = document.querySelector(\".cart-popin-voile\");\n      const CartIcon = document.querySelector(\".header-cart-wrapper .header-cart-btn\");\n      const CartClose = document.querySelector(\".cart-close\");\n      CartIcon.addEventListener(\"click\", ToggleClassCart);\n      CartClose.addEventListener(\"click\", ToggleClassCart);\n      CartVoile.addEventListener(\"click\", ToggleClassCart);\n\n      function ToggleClassCart() {\n        if (CartContainer.classList.contains(\"cart-is-active\")) {\n          CartContainer.classList.remove(\"cart-is-active\");\n          CartIcon.classList.remove(\"cart-is-active\");\n        } else {\n          CartContainer.classList.add(\"cart-is-active\");\n          CartIcon.classList.add(\"cart-is-active\");\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,eAAe;AACbA,MAAI,EAAE,WADO;AAEbC,SAAO,EAAE,YAAY;AACnB,SAAKC,UAAL;AACD,GAJY;AAKbC,SAAO,EAAE;AACP;AACAD,cAAU,GAAG;AACX,YAAME,aAAY,GAAIC,QAAQ,CAACC,aAAT,CAAuB,uBAAvB,CAAtB;AACA,YAAMC,SAAQ,GAAIF,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,CAAlB;AACA,YAAME,QAAO,GAAIH,QAAQ,CAACC,aAAT,CACf,uCADe,CAAjB;AAGA,YAAMG,SAAQ,GAAIJ,QAAQ,CAACC,aAAT,CAAuB,aAAvB,CAAlB;AAEAE,cAAQ,CAACE,gBAAT,CAA0B,OAA1B,EAAmCC,eAAnC;AACAF,eAAS,CAACC,gBAAV,CAA2B,OAA3B,EAAoCC,eAApC;AACAJ,eAAS,CAACG,gBAAV,CAA2B,OAA3B,EAAoCC,eAApC;;AAEA,eAASA,eAAT,GAA2B;AACzB,YAAIP,aAAa,CAACQ,SAAd,CAAwBC,QAAxB,CAAiC,gBAAjC,CAAJ,EAAwD;AACtDT,uBAAa,CAACQ,SAAd,CAAwBE,MAAxB,CAA+B,gBAA/B;AACAN,kBAAQ,CAACI,SAAT,CAAmBE,MAAnB,CAA0B,gBAA1B;AACF,SAHA,MAGO;AACLV,uBAAa,CAACQ,SAAd,CAAwBG,GAAxB,CAA4B,gBAA5B;AACAP,kBAAQ,CAACI,SAAT,CAAmBG,GAAnB,CAAuB,gBAAvB;AACF;AACF;AACD;;AAvBM;AALI,CAAf","names":["name","mounted","ToogleCart","methods","CartContainer","document","querySelector","CartVoile","CartIcon","CartClose","addEventListener","ToggleClassCart","classList","contains","remove","add"],"sourceRoot":"","sources":["/Users/hugocrozet/Documents/Website/test-technique/src/components/CartPopin.vue"],"sourcesContent":["<template>\n  <div class=\"cart-popin-container\">\n    <div class=\"cart-popin-voile\"></div>\n    <div class=\"cart-popin-wrapper\">\n      <div class=\"cart-header\">\n        <h2>Order Summary</h2>\n        <p class=\"cart-close\">Close</p>\n      </div>\n      <div class=\"carte-order-summary\">\n        <p>Your Cart is empty</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CartPopin\",\n  mounted: function () {\n    this.ToogleCart();\n  },\n  methods: {\n    // Toggle Cart Interface\n    ToogleCart() {\n      const CartContainer = document.querySelector(\".cart-popin-container\");\n      const CartVoile = document.querySelector(\".cart-popin-voile\");\n      const CartIcon = document.querySelector(\n        \".header-cart-wrapper .header-cart-btn\"\n      );\n      const CartClose = document.querySelector(\".cart-close\");\n\n      CartIcon.addEventListener(\"click\", ToggleClassCart);\n      CartClose.addEventListener(\"click\", ToggleClassCart);\n      CartVoile.addEventListener(\"click\", ToggleClassCart);\n\n      function ToggleClassCart() {\n        if (CartContainer.classList.contains(\"cart-is-active\")) {\n          CartContainer.classList.remove(\"cart-is-active\");\n          CartIcon.classList.remove(\"cart-is-active\");\n        } else {\n          CartContainer.classList.add(\"cart-is-active\");\n          CartIcon.classList.add(\"cart-is-active\");\n        }\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"css\" src=\"@/assets/css/cart_popin.css\"></style>\n"]},"metadata":{},"sourceType":"module"}