e\" />\n        <label for=\"menu-toggle\" class=\"menu-open\">\n          <div id=\"open\" class=\"home-burger\"></div>\n        </label>\n        <div class=\"menu-mobile menu-effects\">\n          <ul>\n            <li>\n              <router-link to=\"#Homme\">Homme</router-link>\n            </li>\n            <li>\n              <router-link to=\"#Femme\">Femme</router-link>\n            </li>\n            <li>\n              <router-link to=\"#Concept\">Concept</router-link>\n            </li>\n          </ul>\n        </div>\n      </div>\n      <div class=\"header-logo-wrapper\">\n        <router-link to=\"#Logo\">\n          <img\n            src=\"@/assets/images/logo.svg\"\n            alt=\"Logo The Kooples Second Love\"\n          />\n        </router-link>\n      </div>\n      <div class=\"header-cart-wrapper\">\n        <router-link to=\"#Cart\">\n          <img\n            src=\"@/assets/images/icon-cart.png\"\n            alt=\"Logo The Kooples Second Love\"\n          />\n          <p class=\"cart-quantity\">{{ cart }}</p>\n        </router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { gsap } from \"gsap\";\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\n\ngsap.registerPlugin(ScrollTrigger);\n\nexport default {\n  name: \"BannerLarge\",\n\n  data: () => ({\n    cart: 0,\n  }),\n\n  mounted() {\n    this.MobileCheckedMenuAdaptation();\n    this.ChangeColorHeader();\n  },\n  methods: {\n    MobileCheckedMenuAdaptation: function () {\n      const MenuToggle = document.querySelector(\"#menu-toggle\");\n      const MenuBtn = document.querySelector(\".menu-open\");\n      const HeaderLogo = document.querySelector(\".header-logo-wrapper a img\");\n      const HeaderCart = document.querySelector(\".header-cart-wrapper a img\");\n\n      MenuBtn.addEventListener(\"click\", toggleCheckedClass);\n\n      function toggleCheckedClass() {\n        console.log(\"test\");\n        if (MenuToggle.checked) {\n          HeaderLogo.classList.remove(\"checked\");\n          HeaderCart.classList.remove(\"checked\");\n        } else {\n          HeaderLogo.classList.add(\"checked\");\n          HeaderCart.classList.add(\"checked\");\n        }\n      }\n    },\n    ChangeColorHeader: function () {\n      const HeaderContainer = document.querySelector(\".header-container\");\n      const LandingSection = document.querySelector(\".banner-large-section\");\n      gsap.to(HeaderContainer, {\n        scrollTrigger: {\n          trigger: LandingSection,\n          start: \"top bottom\",\n          end: \"bottom 2%\",\n          toggleClass: { targets: HeaderContainer, className: \"is-active\" },\n        },\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"css\" src=\"@/assets/css/header.css\"></style>\n"]},"metadata":{},"sourceType":"module"}