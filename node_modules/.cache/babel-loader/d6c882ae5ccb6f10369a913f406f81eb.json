{"ast":null,"code":"import { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\nexport default {\n  name: \"BannerLarge\",\n  computed: { ...mapState([\"products\"])\n  },\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n  methods: {\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      console.log(this.products.length);\n\n      if (this.products.length === 0) {\n        console.log(\"0\");\n        const truc = this;\n        const myTimeout = setTimeout(myGreeting, 5000);\n\n        function myGreeting() {\n          if (truc.products.length !== 0) {\n            new Swiper(\".swiper\", {\n              loop: true,\n              speed: 1000,\n              //noSwiping: true,\n              //noSwipingClass: \"swiper-wrapper\",\n              slidesPerView: 4,\n              spaceBetween: 20\n            });\n          } else {\n            myTimeout;\n          }\n        }\n      } else {\n        console.log(\"not 0\");\n        new Swiper(\".swiper\", {\n          loop: true,\n          speed: 1000,\n          //noSwiping: true,\n          //noSwipingClass: \"swiper-wrapper\",\n          slidesPerView: 4,\n          spaceBetween: 20\n        });\n      }\n    },\n\n    ...mapActions([\"fetchAllProducts\"])\n  },\n\n  created() {\n    this.fetchAllProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAyCA,SAASA,UAAT,EAAqBC,QAArB,QAAqC,MAArC;AACA,OAAOC,MAAP,MAAmB,QAAnB;AACA,OAAO,YAAP;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAGbC,UAAQ,EAAE,EACR,GAAGH,QAAQ,CAAC,CAAC,UAAD,CAAD;AADH,GAHG;AAObI,SAAO,EAAE,YAAY;AACnB,SAAKC,eAAL;AACD,GATY;AAWbC,SAAO,EAAE;AACP;AACAD,mBAAe,GAAG;AAChBE,aAAO,CAACC,GAAR,CAAY,KAAKC,QAAL,CAAcC,MAA1B;;AACA,UAAI,KAAKD,QAAL,CAAcC,MAAd,KAAyB,CAA7B,EAAgC;AAC9BH,eAAO,CAACC,GAAR,CAAY,GAAZ;AAEA,cAAMG,IAAG,GAAI,IAAb;AAEA,cAAMC,SAAQ,GAAIC,UAAU,CAACC,UAAD,EAAa,IAAb,CAA5B;;AAEA,iBAASA,UAAT,GAAsB;AACpB,cAAIH,IAAI,CAACF,QAAL,CAAcC,MAAd,KAAyB,CAA7B,EAAgC;AAC9B,gBAAIT,MAAJ,CAAW,SAAX,EAAsB;AACpBc,kBAAI,