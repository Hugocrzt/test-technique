{"ast":null,"code":"import { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\nexport default {\n  name: \"BannerLarge\",\n  computed: { ...mapState([\"products\"])\n  },\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n  methods: {\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      console.log(this.products);\n\n      if (this.products.image === null) {\n        console.log(\"null\");\n        new Swiper(\".swiper\", {\n          loop: true,\n          speed: 1000,\n          slidesPerView: 4,\n          spaceBetween: 20\n        });\n      }\n    },\n\n    ...mapActions([\"fetchAllProducts\"])\n  },\n\n  created() {\n    this.fetchAllProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAyCA,SAASA,UAAT,EAAqBC,QAArB,QAAqC,MAArC;AACA,OAAOC,MAAP,MAAmB,QAAnB;AACA,OAAO,YAAP;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAGbC,UAAQ,EAAE,EACR,GAAGH,QAAQ,CAAC,CAAC,UAAD,CAAD;AADH,GAHG;AAObI,SAAO,EAAE,YAAY;AACnB,SAAKC,eAAL;AACD,GATY;AAWbC,SAAO,EAAE;AACP;AACAD,mBAAe,GAAG;AAChBE,aAAO,CAACC,GAAR,CAAY,KAAKC,QAAjB;;AACA,UAAI,KAAKA,QAAL,CAAcC,KAAd,KAAwB,IAA5B,EAAkC;AAChCH,eAAO,CAACC,GAAR,CAAY,MAAZ;AACA,YAAIP,MAAJ,CAAW,SAAX,EAAsB;AACpBU,cAAI,EAAE,IADc;AAEpBC,eAAK,EAAE,IAFa;AAGpBC,uBAAa,EAAE,CAHK;AAIpBC,sBAAY,EAAE;AAJM,SAAtB;AAMF;AACD,KAbM;;AAcP,OAAGf,UAAU,CAAC,CAAC,kBAAD,CAAD;AAdN,GAXI;;AA4BbgB,SAAO,GAAG;AACR,SAAKC,gBAAL;AACD;;AA9BY,CAAf","names":["mapActions","mapState","Swiper","name","computed","mounted","SwiperCarrousel","methods","console","log","products","image","loop","speed","slidesPerView","spaceBetween","created","fetchAllProducts"],"sourceRoot":"","sources":["/Users/hugocrozet/Documents/Website/test-technique/src/components/BannerCarrousel.vue"],"sourcesContent":["<template>\n  <section class=\"banner-caroussel-section\">\n    <div class=\"banner-caroussel-container\">\n      <div class=\"subtitle-left-wrapper\">\n        <h2>Notre Sélection</h2>\n      </div>\n      <div class=\"banner-caroussel-wrapper\">\n        <div class=\"swiper product-container\">\n          <div class=\"swiper-wrapper products-wrapper\">\n            <div\n              v-for=\"product in products\"\n              :key=\"product.id\"\n              class=\"swiper-slide\"\n            >\n              <router-link to=\"#product\" class=\"products-space-image\">\n                <img class=\"product-image\" :src=\"product.image\" alt=\"\" />\n                <img\n                  class=\"rollover-product-image\"\n                  :src=\"product.image\"\n                  alt=\"\"\n                />\n              </router-link>\n              <div class=\"products-space-text\">\n                <router-link to=\"#product\"\n                  ><p class=\"product-title\">{{ product.title }}</p></router-link\n                >\n                <router-link to=\"#product\"\n                  ><p class=\"product-price\">\n                    {{ product.price }} €\n                  </p></router-link\n                >\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport Swiper from \"swiper\";\nimport \"swiper/css\";\n\nexport default {\n  name: \"BannerLarge\",\n\n  computed: {\n    ...mapState([\"products\"]),\n  },\n\n  mounted: function () {\n    this.SwiperCarrousel();\n  },\n\n  methods: {\n    // SwiperCarrousel Init\n    SwiperCarrousel() {\n      console.log(this.products);\n      if (this.products.image === null) {\n        console.log(\"null\");\n        new Swiper(\".swiper\", {\n          loop: true,\n          speed: 1000,\n          slidesPerView: 4,\n          spaceBetween: 20,\n        });\n      }\n    },\n    ...mapActions([\"fetchAllProducts\"]),\n  },\n\n  created() {\n    this.fetchAllProducts();\n  },\n};\n</script>\n\n<style lang=\"css\" src=\"@/assets/css/banner_carrousel.css\"></style>\n"]},"metadata":{},"sourceType":"module"}