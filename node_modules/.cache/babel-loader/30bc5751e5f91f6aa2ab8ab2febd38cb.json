{"ast":null,"code":"import BannerLarge from \"@/components/BannerLarge\";\nimport BannerFaq from \"@/components/BannerFaq\";\nimport CartPopin from \"@/components/CartPopin\";\nimport { gsap } from \"gsap\";\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"HomePage\",\n  components: {\n    BannerLarge,\n    BannerFaq,\n    CartPopin\n  },\n\n  mounted() {\n    window.scrollTo(0, 0);\n    this.ChangeColorHeader();\n  },\n\n  methods: {\n    // Toggle Menu Color Landing\n    ChangeColorHeader() {\n      window.scrollTo(0, 0);\n      ScrollTrigger.refresh(true);\n      const HeaderContainer = document.querySelector(\".header-container\");\n      const LandingSection = document.querySelector(\".banner-large-section\");\n      gsap.to(HeaderContainer, {\n        scrollTrigger: {\n          trigger: LandingSection,\n          start: \"top bottom\",\n          end: \"bottom 0%\",\n          toggleClass: {\n            targets: HeaderContainer,\n            className: \"is-active\"\n          }\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA,OAAOA,WAAP,MAAwB,0BAAxB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AACA,SAASC,IAAT,QAAqB,MAArB;AACA,SAASC,aAAT,QAA8B,oBAA9B;AAEAD,IAAI,CAACE,cAAL,CAAoBD,aAApB;AAEA,eAAe;AACbE,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVP,eADU;AAEVC,aAFU;AAGVC;AAHU,GAFC;;AAObM,SAAO,GAAG;AACRC,UAAM,CAACC,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;AACA,SAAKC,iBAAL;AACD,GAVY;;AAWbC,SAAO,EAAE;AACP;AACAD,qBAAiB,GAAG;AAClBF,YAAM,CAACC,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;AACAN,mBAAa,CAACS,OAAd,CAAsB,IAAtB;AACA,YAAMC,eAAc,GAAIC,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,CAAxB;AACA,YAAMC,cAAa,GAAIF,QAAQ,CAACC,aAAT,CAAuB,uBAAvB,CAAvB;AACAb,UAAI,CAACe,EAAL,CAAQJ,eAAR,EAAyB;AACvBK,qBAAa,EAAE;AACbC,iBAAO,EAAEH,cADI;AAEbI,eAAK,EAAE,YAFM;AAGbC,aAAG,EAAE,WAHQ;AAIbC,qBAAW,EAAE;AAAEC,mBAAO,EAAEV,eAAX;AAA4BW,qBAAS,EAAE;AAAvC;AAJA;AADQ,OAAzB;AAQD;;AAfM;AAXI,CAAf","names":["BannerLarge","BannerFaq","Cart